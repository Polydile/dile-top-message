{
  "name": "dile-top-message",
  "version": "1.0.2",
  "description": "Webcomponent dile-top-message following open-wc recommendations",
  "author": "dile-top-message",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "start": "es-dev-server --app-index demo/index.html --node-resolve --open --watch",
    "start:compatibility": "es-dev-server --app-index demo/index.html --compatibility all --node-resolve --open --watch",
    "site:build": "npm run storybook:build",
    "storybook": "start-storybook -p 9001",
    "storybook:build": "build-storybook -o _site -s storybook-static"
  },
  "dependencies": {
    "dile-close-icon-template": "^1.0.3",
    "dile-slide-down-mixin": "^1.0.1",
    "lit-element": "^2.0.1",
    "lit-html": "^1.0.0"
  },
  "devDependencies": {
    "es-dev-server": "^1.5.0",
    "@open-wc/demoing-storybook": "^0.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Polydile/dile-top-message.git"
  }
}
